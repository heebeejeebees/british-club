<%- include('../partials/head') -%> 
<style>
  .flex-parent {
    display: flex;
    justify-content: center;
    align-items: center;
  }

  .box {
    border: 2px solid black;
    margin: 20px;
    padding: 20px;
  }
</style>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark">Member Details</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Member Details</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>

    <!-- /.content-header -->
    <div class="content">
      <div class="container-fluid">
        <div class="row">
            <div class="col-12 text-center">
                <% if(user.pp) { %>
                    <img src="data:image/png;base64,<%= user.pp %>" class="img-circle elevation-2" alt="User Image" style="width: 11rem; height: 11rem;">
                <% } else{ %>  
                    <img src="data:image/png;base64," class="img-circle elevation-2" alt="User Image" style="width: 11rem; height: 11rem;">
                <% } %>
            </div>
            <div class="col-12 text-center">
                <label style="color:blue;font-size:18px;"> <%= data.Name %></label>
            </div>
            <div class="col-6">
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title">Personal Information</h5>
                    </div>
                    <div class="card-body">
                      <form>
                        <div class="form-group">
                          <label for="fullName">Full Name</label>
                          <input type="email" class="form-control" id="fullName" aria-describedby="fullName" placeholder="Enter email" value="<%= data.Name %>" readonly>
                        </div>
                        <div class="form-group">
                            <label for="dob">Date of Birth</label>
                            <input type="dob" class="form-control" id="dob" aria-describedby="dob" placeholder="Date of birth" value="<%= data.Birth %>" readonly>
                          </div>
                          <div class="form-group">
                            <label for="nationality">Nationality</label>
                            <input type="nationality" class="form-control" id="nationality" aria-describedby="nationality" placeholder="Nationality" value="<%= data.Nationality %>" readonly>
                          </div>
                      </form>
                    </div>
                </div>
            </div>
            <div class="col-6">
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title">Membership Information</h5>
                    </div>
                    <div class="card-body">
                      <form>
                        <div class="form-group">
                          <label for="mshipType">Membership Type</label>
                          <input type="text" class="form-control" id="mshipType" aria-describedby="mshipType" placeholder="Membership Type" value="<%= data.MembershipType %>" readonly>
                        </div>
                        <div class="form-group">
                            <label for="jd">Join Date</label>
                            <input type="text" class="form-control" id="jd" aria-describedby="jd" placeholder="Join Date" value="<%= data.JoinDate %>" readonly>
                        </div>
                        <div class="form-group">
                            <label for="cardType">Card Type</label>
                            <input type="text" class="form-control" id="cardType" aria-describedby="cardType" placeholder="Card Type" value="<%= data.CardType %>" readonly>
                        </div>
                        <div class="form-group">
                            <label for="expiryDate">Expiry Date</label>
                            <input type="text" class="form-control" id="expiryDate" aria-describedby="expiryDate" placeholder="Expiry Date" value="<%= data.ExpiryDate %>" readonly>
                        </div>
                      </form>
                    </div>
                </div>
            </div>
            <div class="col-6">
              <form role="form" action="/portal/memberdetails/submit" method="POST"> 
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title">Personal Contact Details</h5>
                    </div>
                    <div class="card-body">
                        <div class="form-group">
                          <label for="fname">Phone Number</label>
                          <input type="text" class="form-control" id="Phone" name="Phone" value="<%= HomeD.Mobile %>">
                        </div>
                        <div class="form-group">
                            <label for="femail">Personal Email</label>
                            <input type="text" class="form-control" id="Email" name="Email" value="<%= HomeD.Email %>">
                        </div>
                        <div class="form-group">
                            <label>Address</label>
                            <br><input type="text" class="form-control" id="Address1" name="Address1" value="<%= HomeD.Address1 %>">
                            <br><input type="text" class="form-control" id="Address2" name="Address2" value="<%= HomeD.Address2 %>">
                        </div>
                        <div class="form-group">
                            <label>City</label>
                            <br><input type="text" class="form-control" id="City" name="City" value="<%= HomeD.City %>">
                        </div>
                        <div class="form-group">
                            <label>Postal</label>
                            <br><input type="text" class="form-control" id="Postal" name="Postal" value="<%= HomeD.Postal %>">
                        </div>
                        <div class="form-group">
                            <label>Country</label>
                            <br><input type="text" class="form-control" id="Country" name="Country" value="<%= HomeD.Country %>">
                        </div>
                    </div>
                    <div class="card-footer">
                        <input type="hidden" id="id" name="id" value="<%= data.MemberID %>" class="form-control">
                        <button type="submit" name="action" value="personal_details" class="btn btn-dark float-right">Submit</button>
                    </div>
                </div>
              </form>
            </div>
            <div class="col-6">
              <form role="form" action="/portal/memberdetails/submit" method="POST">
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title">Company Details</h5>
                    </div>
                    <div class="card-body">
                        <div class="form-group">
                            <label for="fname">Company Name</label>
                            <input type="text" class="form-control" id="Company" name="Company" value="<%= WorkD.Company %>">
                        </div>
                        <div class="form-group">
                            <label for="fname">Work Title</label>
                            <input type="text" class="form-control" id="Work" name="Work" value="<%= WorkD.Job %>">
                        </div>
                        <div class="form-group">
                          <label for="fname">Phone Number</label>
                          <input type="text" class="form-control" id="WPhone" name="WPhone" value="<%= WorkD.Mobile %>">
                        </div>
                        <div class="form-group">
                            <label for="femail">Work Email</label>
                            <input type="text" class="form-control" id="WEmail" name="WEmail" value="<%= WorkD.Email %>">
                        </div>
                        <div class="form-group">
                            <label>Address</label>
                            <br><input type="text" class="form-control" id="WAddress1" name="WAddress1" value="<%= WorkD.Address1 %>">
                            <br><input type="text" class="form-control" id="WAddress2" name="WAddress2" value="<%= WorkD.Address2 %>">
                        </div>
                        <div class="form-group">
                            <label>City</label>
                            <br><input type="text" class="form-control" id="WCity" name="WCity" value="<%= WorkD.City %>">
                        </div>
                        <div class="form-group">
                            <label>Postal</label>
                            <br><input type="text" class="form-control" id="WPostal" name="WPostal" value="<%= WorkD.Postal %>">
                        </div>
                        <div class="form-group">
                            <label>Country</label>
                            <br><input type="text" class="form-control" id="WCountry" name="WCountry" value="<%= WorkD.Country %>">
                        </div>
                    </div>
                    <div class="card-footer">
                        <input type="hidden" id="id" name="id" value="<%= data.MemberID %>" class="form-control">
                        <button type="submit" name="action" value="company_details" class="btn btn-dark float-right">Submit</button>
                    </div>
                </div>
              </form>
            </div>
            <div class="col-6">
              <form role="form" action="/portal/memberdetails/submit" method="POST">
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title">Preferences</h5>
                    </div>
                    <div class="card-body">
                        <table class="table">
                        <tr>
                            <th></th>
                            <th>Mobile<br>Number</th>
                            <th>Personal<br>Email</th>
                            <th>Work<br>Phone</th>
                            <th>Work<br>Email</th>
                        </tr>
                        <tr>
                            <td>Primary Contact</td>   
                            <%  var sa1="" %>
                            <%  var sa2="" %>
                            <%  var sa3="" %>
                            <%  var sa4="" %>
                
                            <%  if(prefP.Phone == 1 ) { sa1 = 'checked'  } else {  sa = ' ' } %>
                            <%  if(prefP.Email == 1 ) { sa2 = 'checked'  } else {  sa = ' ' } %>
                            <%  if(prefP.Work == 1 ) { sa3 = 'checked'  } else {  sa = ' ' } %>
                            <%  if(prefP.WEmail == 1 ) { sa4 = 'checked'  } else {  sa = ' ' } %>
                
                            <td><input type="radio" id="v1" name="primary" value="personal_mobile" onclick="myFunction()" <%= sa1 %>></td>
                            <td><input type="radio" id="v2" name="primary" value="personal_email" onclick="myFunction2()" <%= sa2 %>></td>
                            <td><input type="radio" id="v3" name="primary" value="work_mobile" onclick="myFunction3()" <%= sa3 %>></td>
                            <td><input type="radio" id="v4" name="primary" value="work_email" onclick="myFunction4()"<%= sa4 %>></td>
                        </tr>
                        <tr>
                            <td>Correspondence</td>
                            <%  var Ca1="" %>
                            <%  var Ca2="" %>
                            <%  var Ca3="" %>
                            <%  var Ca4="" %>
                
                            <%  if(prefC.Phone == 1 ) { Ca1 = 'checked'  } else {  sa = ' ' } %>
                            <%  if(prefC.Email == 1 ) { Ca2 = 'checked'  } else {  sa = ' ' } %>
                            <%  if(prefC.Work == 1 ) { Ca3 = 'checked'  } else {  sa = ' ' } %>
                            <%  if(prefC.WEmail == 1 ) { Ca4 = 'checked'  } else {  sa = ' ' } %>
                
                            <td><input type="radio" id="c1" name="correspondence" value="personal_mobile" onclick="myF1()" <%= Ca1 %>></td>
                            <td><input type="radio" id="c2" name="correspondence" value="personal_email" onclick="myF2()" <%= Ca2 %>></td>
                            <td><input type="radio" id="c3" name="correspondence" value="work_mobile" onclick="myF3()" <%= Ca3 %>></td>
                            <td><input type="radio" id="c4" name="correspondence" value="work_email" onclick="myF4()" <%= Ca4 %>></td>
                        </tr>
                        </table>
                    </div>
                    <div class="card-footer">
                      <input type="hidden" id="id" name="id" value="<%= data.MemberID %>" class="form-control">
                      <button type="submit" name="action" value="preferences" class="btn btn-dark float-right">Submit</button>
                    </div>
                </div>
              </form>
            </div>
            <div class="col-6">
                &nbsp;
            </div>
          </tr>
        </div>
      </div>
    </div>
  </div>


